{
  "rust-analyzer.serverPath": "/etc/profiles/per-user/elf/bin/rust-analyzer",
  "rust-analyzer.lens.run": false,
  "rust-analyzer.debug.runtime": "vimspector",
  "tsserver.npm": "/etc/profiles/per-user/elf/bin/npm",
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.snippets.enable": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": false,
  "codeLens.enable": true,
  "diagnostic.warningSign": "⚑",
  "diagnostic.errorSign": "✘",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "ℹ",
  "coc.preferences.formatOnSaveFiletypes": ["rust"]
}
